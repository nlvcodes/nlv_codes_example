{
  "name": "blank-payload",
  "version": "1.0.0",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start"
  },
  "dependencies": {
    "@payloadcms/db-mongodb": "3.37.0",
    "@payloadcms/email-nodemailer": "3.37.0",
    "@payloadcms/email-resend": "3.37.0",
    "@payloadcms/live-preview-react": "3.37.0",
    "@payloadcms/next": "3.37.0",
    "@payloadcms/plugin-form-builder": "3.37.0",
    "@payloadcms/plugin-redirects": "3.37.0",
    "@payloadcms/plugin-seo": "3.37.0",
    "@payloadcms/richtext-lexical": "3.37.0",
    "@payloadcms/storage-s3": "3.37.0",
    "@payloadcms/storage-uploadthing": "3.37.0",
    "@payloadcms/storage-vercel-blob": "3.37.0",
    "@payloadcms/ui": "3.37.0",
    "@radix-ui/react-checkbox": "^1.3.0",
    "@tailwindcss/postcss": "^4.0.9",
    "@types/react-google-recaptcha": "^2.1.9",
    "cross-env": "^7.0.3",
    "escape-html": "^1.0.3",
    "graphql": "^16.8.1",
    "lucide-react": "^0.487.0",
    "next": "15.3.2",
    "payload": "3.37.0",
    "postcss": "^8.5.3",
    "prism-react-renderer": "^2.4.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-google-recaptcha": "^3.1.0",
    "react-hook-form": "^7.56.2",
    "sharp": "0.32.6",
    "sonner": "^2.0.3",
    "tailwindcss": "^4.0.9"
  },
  "devDependencies": {
    "@types/node": "^22.5.4",
    "@types/react": "19.0.1",
    "@types/react-dom": "19.0.1",
    "eslint": "^9.16.0",
    "eslint-config-next": "15.0.4",
    "typescript": "5.7.2"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0"
